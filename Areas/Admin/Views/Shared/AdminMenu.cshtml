<!DOCTYPE html>
<html lang="en-US">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initail-scale=1" />
    <meta charset="utf-8" />
    <title> ICSI IT Inventory Management System </title>
    <!--Google Fonts-->
    <link href="https://fonts.googleapis.com/css?family=Nanum+Gothic" rel="stylesheet">

    <link href="~/CSS/bootstrap.min.css" rel="stylesheet" />
    <!--Material Design Iconic Font-->

    <link href="~/CSS/material-design-iconic-font.min.css" rel="stylesheet" />

    <!--jQuery-->
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"
            integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
            crossorigin="anonymous"></script>

    <!--hs Menu JS-->


    <script src="~/js/jquery.hsmenu.min.js"></script>
    <!-- hs Menu CSS-->

    <link href="~/CSS/hs-menu.min.css" rel="stylesheet" />

    <style type="text/css">

        

        .zui-table-rounded {
            border: none;
        }

            .zui-table-rounded thead th {
                border: none;
                text-shadow: 1px 1px 1px ;
                
            }

                .zui-table-rounded thead th:first-child {
                    border-radius: 0px 0 0 0;
                }

                .zui-table-rounded thead th:last-child {
                    border-radius: 0 10px 0 0;
                }

            .zui-table-rounded tbody td {
                border: none;
                border-top: solid 1px black;
            }

         


        /* top-left border-radius */
            .zui-table-rounded tbody tr:first-child th:first-child {
                border-top-left-radius: 20px;
            }

        /* top-right border-radius */
            .zui-table-rounded tbody tr:first-child th:last-child {
                border-top-right-radius: 20px;
            }

        /* bottom-left border-radius */
            .zui-table-rounded tbody tr:last-child td:first-child {
                border-bottom-left-radius: 20px;
            }

        /* bottom-right border-radius */
            .zui-table-rounded tbody tr:last-child td:last-child {
                border-bottom-right-radius: 20px;
            }



    </style>

    <script>
        $(document).ready(function () {

            $(".hs-menubar").hsMenu();

        });
    </script>

</head>
<body>

    <!--Start hs Mega Menu-->

    <header class="hs-menubar">
        <div class="brand-logo" >
            <a href="#home_link"><img src="~/image/ICSI_Logo.png" style="width:60px; height:38px;" title="Your logo will be here" alt="hs Mega Menu"> </a>
        </div>
        <div class="menu-trigger"> <i class="zmdi zmdi-menu"></i></div>
        <div class="search-trigger"> <i class="zmdi zmdi-search"></i></div>
        <div class="search-box">
            <form action="#">
                <input type="text" name="search">
                <button type="submit" class="search-submit" disabled>Search</button>
            </form>
        </div>
        <div class="hs-user toggle" data-reveal=".user-info">
            <img src="~/image/Icon.jpg" alt="Asif Mughal">
        </div>
        <div class="grid-trigger toggle" data-reveal=".grid-items"> <i class="zmdi zmdi-view-module"></i> </div>
        <div class="more-trigger toggle" data-reveal=".user-penal"> <i class="zmdi zmdi-more-vert"></i></div>
    </header>
    <section class="box-model">
        <ul class="user-penal">
            <li> <a href="#1"><i class="zmdi zmdi-inbox"></i> Inbox </a> </li>
            <li> <a href="#1"> <i class="zmdi zmdi-delete"></i> Trash </a> </li>
            <li> <a href="#1"> <i class="zmdi zmdi-star"></i> Started  </a> </li>
            <li> <a href="#1"> <i class="zmdi zmdi-run"></i> Logout  </a> </li>
        </ul>
        <ul class="user-info">
            <li class="profile-pic"> </li>
            <li class="user-name">Hrisikesh Kumar  </li>
            <li class="user-bio">  Developer</li>
            <li class="more-btn"> <a href="#1"> Find Out More</a> </li>
        </ul>
        <ul class="grid-items">
            <li class="grid"> Item one</li>
            <li class="grid"> Item two </li>
            <li class="grid"> Item three </li>
            <li class="grid"> Item four </li>
            <li class="grid"> Item five </li>
            <li class="grid"> Item six </li>
            <li class="more-btn"><a href="#1"> More</a></li>
        </ul>
    </section>
    <nav class="hs-navigation">
        <ul class="nav-links">
            <li><a href="/Admin/Admin_Dashboard/Admin_Dashboard"> <span class="icon"> <i class="zmdi zmdi-collection-item"></i> </span> Dash Board</a> </li>

            <li><a href="@Url.Action("Employee_Details", null, new { area = "Admin", controller = "Employee" }, Request.Url.Scheme)"> <span class="icon"> <i class="zmdi zmdi-collection-item"></i> </span> Employees Data</a> </li>

            <li><a href="@Url.Action("Vendor_Details", null, new { area = "Admin", controller = "Vendor" }, Request.Url.Scheme)"> <span class="icon"> <i class="zmdi zmdi-collection-item"></i> </span> Vendor Data</a> </li>

            <li><a href="@Url.Action("Get_Master_Data", null, new { area = "Admin", controller = "MasterData" }, Request.Url.Scheme)"> <span class="icon"> <i class="zmdi zmdi-collection-item"></i> </span>Make/Model Master</a> </li>

            <li class="has-child">
                <span class="its-parent">
                    <span class="icon">
                        <i class="zmdi zmdi-device-hub"></i>
                    </span>IT Assets
                </span>
                <ul class="its-children">
                    <li class="has-child">
                        <span class="its-parent"> Computer </span>
                        <ul class="its-children">
                            <li> <a href="@Url.Action("Com_Details", null, new { area = string.Empty, controller = "Computer" }, Request.Url.Scheme)">Desktop</a> </li>
                            <li> <a href="@Url.Action("Monitor_Details", null, new { area = string.Empty, controller = "Monitor" }, Request.Url.Scheme)"> Monitor </a> </li>
                            <li> <a href="@Url.Action("Lap_Details", null, new { area = string.Empty, controller = "Laptop" }, Request.Url.Scheme)"> Laptop </a> </li>
                        </ul>
                    </li>
                    <li class="has-child">
                        <span class="its-parent"> Printer/Scanner / UPS </span>
                        <ul class="its-children">
                            <li> <a href="@Url.Action("Printer_Details", null, new { area = string.Empty, controller = "Printer" }, Request.Url.Scheme)">Printer</a> </li>
                            <li> <a href="@Url.Action("Scanner_Details", null, new { area = string.Empty, controller = "Scanner" }, Request.Url.Scheme)"> Scanner </a> </li>
                            <li> <a href="@Url.Action("UPS_Details", null, new { area = string.Empty, controller = "UPS" }, Request.Url.Scheme)"> UPS </a> </li>
                        </ul>
                    </li>

                    <li class="has-child">
                        <span class="its-parent"> Data Center Items </span>
                        <ul class="its-children">
                            <li> <a href="@Url.Action("Server_Details", null, new { area = string.Empty, controller = "Server" }, Request.Url.Scheme)">Servers</a> </li>
                            <li> <a href="@Url.Action("Switch_Details", null, new { area = string.Empty, controller = "Switch" }, Request.Url.Scheme)"> Switches </a> </li>
                            <li> <a href="@Url.Action("DC_MisItems_Details", null, new { area = string.Empty, controller = "DC_MisItems" }, Request.Url.Scheme)"> Other Items </a> </li>
                        </ul>
                    </li>

                    <li class="has-child">
                        <span class="its-parent">
                            <span class="icon">
                                <i class="zmdi zmdi-wrap-text"></i>
                            </span>Other IT Assets
                        </span>
                        <ul class="its-children">

                            <li> <a href="@Url.Action("DataCard_Details", null, new { area = string.Empty, controller = "DataCard" }, Request.Url.Scheme)">Data Card</a> </li>
                            <li> <a href="@Url.Action("AppleIpad_Details", null, new { area = string.Empty, controller = "AppleIpad" }, Request.Url.Scheme)">Apple Ipad /Surface Pro</a> </li>
                            <li> <a href="@Url.Action("OtherItem_Details", null, new { area = string.Empty, controller = "OtherItem" }, Request.Url.Scheme)"> Mislenious Items</a> </li>

                        </ul>
                    </li>

                </ul>

            </li>
            <li> <a href="@Url.Action("Item_Issue_Details", null, new { area = string.Empty, controller = "Item_Issue" }, Request.Url.Scheme)"> <span class="icon"> <i class="zmdi zmdi-compass"></i> </span> Issue Item To Employees</a>  </li>
            <li> <a href="@Url.Action("Shift_Item_Details", null, new { area = string.Empty, controller = "Shift_Item" }, Request.Url.Scheme)"> <span class="icon"> <i class="zmdi zmdi-compass"></i> </span> Shift Item To Other Company</a>  </li>
            <li> <a href="@Url.Action("Approval_Details", null, new { area = string.Empty, controller = "Approval" }, Request.Url.Scheme)"> <span class="icon"> <i class="zmdi zmdi-compass"></i> </span> Approval</a>  </li>
            <li> <a href="@Url.Action("AMC_Details", null, new { area = string.Empty, controller = "AMC" }, Request.Url.Scheme)"> <span class="icon"> <i class="zmdi zmdi-compass"></i> </span> Item AMC Details</a>  </li>
            <li> <a href="@Url.Action("AMC_Details", null, new { area = string.Empty, controller = "AMC" }, Request.Url.Scheme)"> <span class="icon"> <i class="zmdi zmdi-compass"></i> </span> Other AMC Details</a>  </li>

            <li class="has-child">
                <span class="its-parent">
                    <span class="icon">
                        <i class="zmdi zmdi-wrap-text"></i>
                    </span>Reports
                </span>
                <ul class="its-children">
                    <li> <a href="#1"> Stock Report </a></li>
                    <li> <a href="#1"> Items Movement Report</a></li>

                </ul>
            </li>


            <!--//has-child-->
        </ul>
    </nav>
    <!--End hs Mega Menu-->
    <div class="container" style="padding-top:10%; padding-left:5%">
        @RenderBody()
        
    </div>


</body>
</html>
